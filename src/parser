#!/bin/bash

###
# Parses arguments and commands from shell
###

case "${1}" in
    exclude)
        shift
        cmd_exclude ${@}
    ;;

    include)
        shift
        cmd_include ${@}
    ;;

    deploy)
        shift
        cmd_deploy ${@}
    ;;

    update)
        shift
        cmd_update ${@}
    ;;

    du)
        shift
        cmd_du "${@}"
    ;;
esac

